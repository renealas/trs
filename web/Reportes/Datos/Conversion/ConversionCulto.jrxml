	<queryString>
		<![CDATA[SELECT conversiones.FECHA, conversiones.CONVERSIONES,culto.DESCRIPCION as CULTO, iglesia.DESCRIPCION as IGLESIA, iglesia.COMP
FROM conversiones
INNER JOIN  culto ON conversiones.IDCULTO = culto.IDCULTO
INNER JOIN  iglesia ON conversiones.IDIGLESIA = iglesia.IDIGLESIA
WHERE culto.IDCULTO = ($P{idCulto})
AND conversiones.FECHA BETWEEN $P{fechaInicio} AND $P{fechaFin}]]>
	</queryString>
