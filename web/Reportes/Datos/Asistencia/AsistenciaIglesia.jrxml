	<queryString>
		<![CDATA[SELECT asistencia.FECHA, asistencia.ASISTENCIAADULTOS,  asistencia.ASISTENCIANINOS, iglesia.DESCRIPCION, iglesia.COMP
FROM asistencia 
INNER JOIN  iglesia ON asistencia.IDIGLESIA = iglesia.IDIGLESIA
WHERE iglesia.IDIGLESIA = ($P{idIglesia})
AND asistencia.FECHA BETWEEN $P{fechaInicio} AND $P{fechaFin}]]>
	</queryString>
