	<queryString>
		<![CDATA[SELECT asistencia.FECHA, asistencia.ASISTENCIAADULTOS,  asistencia.ASISTENCIANINOS, culto.DESCRIPCION as CULTO, iglesia.DESCRIPCION as IGLESIA, iglesia.COMP
FROM asistencia 
INNER JOIN  culto ON asistencia.IDCULTO = culto.IDCULTO
INNER JOIN  iglesia ON asistencia.IDIGLESIA = iglesia.IDIGLESIA
WHERE culto.IDCULTO = ($P{idCulto1})
AND asistencia.FECHA BETWEEN $P{fechaInicio} AND $P{fechaFin}]]>
	</queryString>
